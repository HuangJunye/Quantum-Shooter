t=0
function _init()
 ship = {sp=1,x=60,y=60}
end


function _update()
 t=t+1
 if(t%8<4) then
  ship.sp=1
 else
  ship.sp=2
 end
end


function _draw()
 cls()
 spr(ship.sp,ship.x,ship.y)
end
